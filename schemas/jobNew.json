{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "required": ["title", "companyHandle"],
  "additionalProperties": false,
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 90,
      "examples": ["j1"]
    },
    "salary": {
      "type": "integer",
      "minimum": 0,
      "examples": [10]
    },
    "equity": {
      "type": "number",
      "maximum": 1.0,
      "examples": [1.0]
    },
    "companyHandle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30,
      "examples": ["c1"]
    }
  },
  "examples": [
    {
      "title": "j1",
      "salary": 10,
      "equity": 1.0,
      "companyHandle": "c1"
    }
  ]
}
